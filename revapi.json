[
  {
    "extension": "revapi.semver.ignore",
    "configuration": {
      "enabled" : true,
      "versionIncreaseAllows" : {
        "major" : "breaking",
        "minor" : "nonBreaking",
        "patch" : "equivalent"
      },
      "passThroughDifferences": ["java.class.nonPublicPartOfAPI"]
    }
  },
  {
    "extension": "revapi.differences",
    "configuration": {
      "justification": "Allow Testcontainers and Docker libraries to add new methods",
      "ignore": true,
      "differences": [
        {
          "code": "java.method.addedToInterface",
          "newArchive": "org.testcontainers:testcontainers:jar"
        },
        {
          "code": "java.method.addedToInterface",
          "newArchive": "com.github.docker-java:docker-java-core:jar"
        }
      ]
    }
  },
  {
    "extension": "revapi.filter",
    "configuration": {
      "elements": {
        "exclude": [
          {
            "matcher": "java",
            "match": "@org.apiguardian.api.API(status != org.apiguardian.api.API.Status.STABLE) ^*;"
          }
        ]
      }
    }
  }
]
